
@{
    ViewData["Title"] = "Index";
}
<!-- Banner Area Start -->
<div class="banner-area-wrapper">
    <div class="banner-area text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="banner-content-wrapper">
                        <div class="banner-content">
                            <h2>courses</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Banner Area End -->
<!-- Course Start -->
<div class="course-area pt-150 pb-150">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="course-title">
                    <h3>search courses</h3>
                </div>
                <div class="course-form">
                    <form  >
                        <input type="search" placeholder="Search..." id="Course-search" name="search" />
                    </form>
                </div>
            </div>
        </div>
        <div class="row" id="Old-Courses">
            @await Component.InvokeAsync("Course", 9)
        </div>
        <div class="row" id="New-Courses">
        </div>
    </div>
</div>
<!-- Course End -->
